{
  "name": "acquia-pso/dst-entity-generate",
  "description": "Drush powered command line tool for working with Drupal Spec tool.",
  "license": "GPL-2.0+",
  "type": "drupal-module",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Acquia India Team",
      "homepage": "https://www.acquia.com",
      "role": "Maintainer"
    }
  ],
  "require": {
    "cweagans/composer-patches": "^2",
    "drush/drush": "^11 || ^12 || ^13",
    "google/apiclient": "^2.8"
  },
  "require-dev": {
    "acquia/coding-standards": "^1.0.1",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
    "ergebnis/composer-normalize": "^2.8",
    "php-parallel-lint/php-parallel-lint": "^1.4",
    "phpro/grumphp": "^1.16",
    "squizlabs/php_codesniffer": "^3.10"
  },
  "suggest": {},
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpro/grumphp": true,
      "ergebnis/composer-normalize": true
    }
  },
  "extra": {
    "drush": {
      "services": {
        "drush.services.yml": "^9"
      }
    },
    "enable-patching": true
  },
  "scripts": {
    "drupal:validate": "grumphp run"
  }
}
